{
    "name": "marshmallow/nova-field-string-generator",
    "description": "A Laravel Nova field for generating a random string. This can be used for a Text field and for a Password field.",
    "keywords": [
        "laravel",
        "nova",
        "password"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "laravel/nova": "^4.0|^5.0"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Marshmallow\\NovaGenerateString\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Marshmallow\\NovaGenerateString\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
